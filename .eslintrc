{
  "root": true,
  "extends": [
    "plugin:vue/base",
    "plugin:vue/essential",
    "plugin:vue/strongly-recommended",
    "plugin:vue/recommended",
    "@vue/airbnb",
    "@vue/typescript"
  ],
  "rules": {
    "linebreak-style": 0,
    "import/extensions": 0,
    "vue/max-attributes-per-line": 0,
    "no-shadow": 0,
    "arrow-parens": 0,
    "vue/attribute-hyphenation": 0
  }
}
