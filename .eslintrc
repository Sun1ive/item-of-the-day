{
  "root": true,
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:vue/base",
    "plugin:vue/essential",
    "plugin:vue/strongly-recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "import/extensions": 0,
    "linebreak-style": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-underscore-dangle": 0,
    "vue/max-attributes-per-line": 0,
    "no-shadow": 0,
    "import/no-extraneous-dependencies": ["error"]
  }
}
